{
  "name": "medusa",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@4.5.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "nukedb": "docker compose down -v && docker compose up -d",
    "prepare": "medusa db:create --db sdoa && medusa db:migrate && medusa db:sync-links",
    "build": "medusa build",
    "build:prod": "medusa build && mkdir -p build && cp -r .medusa/client/* build/",
    "build:standalone": "medusa build && cd .medusa/server && npm install",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "seed:experience-types": "medusa exec ./src/scripts/seed/experience-types.ts",
    "seed:bento-products": "medusa exec ./src/scripts/seed/bento-products.ts",
    "seed:menus": "medusa exec ./src/scripts/seed-menus.ts",
    "start": "medusa start",
    "dev": "medusa develop",
    "dev:email": "email dev --dir ./src/modules/resend/emails",
    "sync": "medusa db:sync-links",
    "migrate:prod": "medusa db:migrate",
    "start:prod": "medusa db:migrate && medusa start",
    "start:railway": "medusa db:migrate && medusa start",
    "start:standalone": "cd .medusa/server && npm start",
    "seed:prod": "medusa exec ./src/scripts/seed.js && yarn add-user:prod",
    "add-user:prod": "medusa user --email pablo_3@icloud.com --password passw0rd!",
    "migrate": "medusa db:migrate",
    "predeploy": "medusa db:migrate",
    "test:integration:http": "node integration-tests/run-tests-isolated.js",
    "test:integration:http:all": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "tests": "yarn test:integration:http && yarn test:integration:modules",
    "medusa:init": "yarn run nukedb && medusa db:create --db sdoa && yarn run migrate && yarn run sync && yarn run seed:menus && yarn run add-user",
    "reset:prod": "medusa db:drop && medusa db:create --db chefVelez && yarn run migrate && yarn run sync && yarn run seed:menus && yarn run add-user:prod",
    "medusa:init:prod": "npm run migrate:prod && npm run sync",
    "create:medusadb": "medusa db:create --db sdoa",
    "add-user": "medusa user --email admin@medusa-test.com --password supersecret",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@lambdacurry/medusa-product-reviews": "1.2.0",
    "@medusajs/admin-sdk": "2.7.0",
    "@medusajs/cli": "2.7.0",
    "@medusajs/file-s3": "^2.10.2",
    "@medusajs/framework": "2.7.0",
    "@medusajs/icons": "2.8.0",
    "@medusajs/js-sdk": "2.7.0",
    "@medusajs/medusa": "2.7.0",
    "@medusajs/types": "2.7.0",
    "@medusajs/ui": "4.0.14",
    "@mikro-orm/core": "6.4.3",
    "@mikro-orm/knex": "6.4.3",
    "@mikro-orm/migrations": "6.4.3",
    "@mikro-orm/postgresql": "6.4.3",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/instrumentation-express": "^0.54.0",
    "@opentelemetry/instrumentation-http": "^0.205.0",
    "@opentelemetry/instrumentation-pg": "^0.58.0",
    "@opentelemetry/resources": "^2.1.0",
    "@opentelemetry/sdk-node": "^0.205.0",
    "@opentelemetry/sdk-trace-node": "^2.1.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "@react-email/components": "^0.3.2",
    "@sentry/node": "^10.11.0",
    "@sentry/opentelemetry-node": "7.114.0",
    "@types/luxon": "^3.6.2",
    "awilix": "^8.0.1",
    "date-fns": "^4.1.0",
    "medusa-file-s3": "^1.4.2",
    "medusa-plugin-sentry": "^1.5.0",
    "pg": "^8.13.0",
    "react": "^18.2.0",
    "react-big-calendar": "^1.19.4",
    "react-dom": "^18.2.0",
    "resend": "^4.7.0"
  },
  "devDependencies": {
    "@medusajs/test-utils": "2.7.0",
    "@mikro-orm/cli": "6.4.3",
    "@mikro-orm/core": "6.4.3",
    "@mikro-orm/migrations": "6.4.3",
    "@mikro-orm/postgresql": "6.4.3",
    "@react-email/preview-server": "4.2.4",
    "@stdlib/number-float64-base-normalize": "0.0.8",
    "@swc/core": "1.5.7",
    "@swc/jest": "^0.2.36",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.12",
    "@types/mime": "1.3.5",
    "@types/node": "^17.0.8",
    "@types/react": "^18.3.2",
    "@types/react-big-calendar": "^1",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react-email": "^4.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "yalc": "^1.0.0-pre.53"
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "engines": {
    "node": ">=20"
  }
}
